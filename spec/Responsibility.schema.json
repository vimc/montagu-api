{
    "id": "Responsibility",
    "type": "object",
    "properties": {
        "scenario": { "$ref": "Scenario.schema.json" },
        "status": { "$ref": "ResponsibilityStatus.schema.json" },
        "problems": { "$ref": "ProblemsArray.schema.json" },
        "current_estimate": {
            "type": [ "integer", "null" ]
        },
        "countries": { 
            "type": "array",
            "items": { "$ref": "CountryId.schema.json" }
        },
        "years": { "$ref": "YearRange.schema.json" }
    },
    "additionalProperties": false,
    "required": [ 
        "scenario", 
        "status", 
        "problems", 
        "current_estimate"
    ]
}
