{
    "id": "DemographicDataSet",
    "type": "object",
    "properties": {
        "id": { "$ref": "StandardIdentifier.schema.json" },
        "name": { "type": "string" },
        "source": { "anyOf": [ {"type": "string"}, {"type": "null"} ]},
        "sources": {"anyOf": [{ "type": "array", "items": { "type" : "string" } }, { "type": "null" } ]},
        "countries": {"anyOf" : [{ "type": "array", "items": { "$ref": "CountryId.schema.json"  } }, {"type" : "null"} ]},
        "gender_is_applicable": { "type": "boolean" }
    },
    "additionalProperties": false,
    "required": [ "id", "name", "gender_is_applicable" ]
}