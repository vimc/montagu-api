{
    "id": "DemographicDataSet",
    "type": "object",
    "properties": {
        "id": { "$ref": "StandardIdentifier.schema.json" },
        "name": { "type": "string" },
        "source": {"anyOf": ["string", "null"]},
        "sources": {"anyOf": [{ "type": "array", "items": { "type" : "string" } }, "null" ]},
        "countries": {"anyOf" : [{ "type": "array", "items": { "$ref": "CountryId.schema.json"  } }, "null" ]},
        "gender_is_applicable": { "type": "boolean" }
    },
    "additionalProperties": false,
    "required": [ "id", "name", "gender_is_applicable" ]
}