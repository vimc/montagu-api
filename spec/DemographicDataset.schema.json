{
    "id": "DemographicDataSet",
    "type": "object",
    "properties": {
        "id": { "$ref": "StandardIdentifier.schema.json" },
        "name": { "type": "string" },
        "source": { "type": "string" },
        "countries": { "type": "array", "items": { "$ref": "CountryId.schema.json"  } },
        "gender_is_applicable": { "type": "boolean" }
    },
    "additionalProperties": false,
    "required": [ "id", "source", "name", "gender_is_applicable", "countries" ]
}