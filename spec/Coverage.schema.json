{
    "id": "Coverage",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "country":  { "$ref": "CountryId.schema.json" },
            "data": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "year": { "$ref": "Year.schema.json" },
                        "coverage": { 
                            "type": "number",
                            "minimum": 0,
                            "maximum": 100
                        }
                    },
                    "additionalProperties": false,
                    "required": [ "year", "coverage" ]
                }
            }
        },
        "additionalProperties": false,
        "required": [ "country", "data" ]
    }
}